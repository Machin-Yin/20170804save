QPushButton#backBtn {
    border-image: url(:/image/return.png);
}
QPushButton#nextBtn {
    border-image: url(:/image/next.png);
}
QPushButton#refreshBtn {
    border-image: url(:/image/refresh.png);
}
QPushButton#btnStatus {
    border:1px groove;
    border-radius:2px;
    border-color:#c8c8c8

}
/*QPushButton#btnStatus:pressed {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #dadbde, stop: 1 #f6f7fa);
}*/
QPushButton#btnStatus:hover{
    background: #1E90FF;
}

QLineEdit#searchBox{
    background-color:transparent;
}


QFrame#searchFrm{
    background-color: rgb(255, 255, 255);
    border: 0px solid;
}

