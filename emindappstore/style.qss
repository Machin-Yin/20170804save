QScrollArea{
    background:transparent;
}

QScrollBar::vertical{
    border:0px solid grey;width:6px;margin-bottom:5px;
}

QScrollBar::handle:vertical{
    border-radius:3px;background:#D0D0D0;
}

QScrollBar::handle:vertical:hover{
    background:#969696;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical{
    background:#F2F2F2;
}
